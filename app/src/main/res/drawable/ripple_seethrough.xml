<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?attr/tintDarkColor"
    >
    <item android:name="android:mask">
        <shape
            android:shape="rectangle">
            <stroke android:color="?attr/tintDarkColor"
                android:width="2dp">
            </stroke>
